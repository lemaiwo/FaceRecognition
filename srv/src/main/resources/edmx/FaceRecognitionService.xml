<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx" xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata" xmlns:sap="http://www.sap.com/Protocols/SAPData">
  <edmx:DataServices m:DataServiceVersion="2.0">
    <Schema Namespace="FaceRecognitionService" xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
      <EntityContainer Name="EntityContainer" m:IsDefaultEntityContainer="true">
        <EntitySet Name="Face" EntityType="FaceRecognitionService.Face"/>
        <FunctionImport Name="getVectorById" EntitySet="Face" ReturnType="FaceRecognitionService.Face" m:HttpMethod="GET">
          <Parameter Name="id" Type="Edm.Int32" Mode="In"/>
        </FunctionImport>
        <FunctionImport Name="compareVectors" EntitySet="Face" ReturnType="FaceRecognitionService.Face" m:HttpMethod="POST">
          <Parameter Name="NewVector" Type="Edm.String" Mode="In"/>
        </FunctionImport>
      </EntityContainer>
      <EntityType Name="Face">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Firstname" Type="Edm.String"/>
        <Property Name="Lastname" Type="Edm.String"/>
        <Property Name="Vectors" Type="Edm.String"/>
        <Property Name="Image" Type="Edm.String"/>
      </EntityType>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>